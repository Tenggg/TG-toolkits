{
  "name": "tg-toolkits",
  "version": "1.0.0",
  "description": "javaScript toolkits",
  "main": "lib/tg-utils.cjs.js",
  "module": "lib/tg-utils.esm.js",
  "umd": "lib/tg-utils.umd.js",
  "types": "./es/index.d.ts",
  "author": "Tenggg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tenggg/tg-toolkits"
  },
  "keywords": [
    "javaScript",
    "utils",
    "toolkits"
  ],
  "files": [
    "dist",
    "lib",
    "src",
    "es"
  ],
  "scripts": {
    "dev": "npx rollup -c",
    "build": "tsc && npx rollup -c --environment NODE_ENV:production",
    "publish:patch": "tsc && npm run build && npm version patch && npm publish",
    "publish:minor": "tsc && npm run build && npm version minor && npm publish",
    "publish:major": "tsc && npm run build && npm version major && npm publish",
    "publish:premajor": "tsc && npm run build && npm version premajor && npm publish",
    "lint": "eslint --ext js,ts  --max-warnings 5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime-corejs3": "^7.17.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.6",
    "prettier": "^2.5.1",
    "rollup": "^2.70.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "typescript": "^4.6.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
